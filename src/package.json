    "autoprefixer-stylus": "^0.8.0",
    "babel": "^5.8.23",
    "babel-core": "^5.8.25",
    "babel-loader": "^5.3.2",
    "babel-runtime": "^5.8.25",
    "classnames": "^2.1.5",
    "css-loader": "^0.19.0",
    "history": "^1.12.4",
    "immutable": "^3.7.5",
    "json-loader": "^0.5.3",
    "postcss-loader": "^0.9.1",
    "react": "^15.1.0",
    "react-bootstrap": "^0.29.4",
    "react-custom-scrollbars": "^4.0.0-beta.2",
