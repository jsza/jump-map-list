    "react-lazyload": "^2.1.1",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.0-alpha.1",
    "react-router-bootstrap": "^0.23.0",
    "react-router-redux": "^4.0.4",
    "react-timeago": "^3.0.0",
    "redux": "^3.5.2",
    "redux-logger": "^2.0.3",
    "redux-router": "^2.0.0-beta1",
    "redux-thunk": "^1.0.0",
    "steamidconvert": "^0.2.4",
    "style-loader": "^0.13.1",
    "stylus": "^0.52.4",
